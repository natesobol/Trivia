		'Log(map_questionText.Get(i))
		List1.Add(i+1)
	Next
	List1 = ShuffleList(List1)
	Log("LIST SIZE: " & List1.Size)
End Sub

' randomizes the order of the questions
Sub ShuffleList(pl As List) As List
	Log("Starting Shufflelist Function.")
	For c = pl.Size - 1 To 0 Step -1
		Dim j As Int
		Dim k As Object
		j = Rnd(0, c + 1)
		k = pl.Get(j)
		pl.Set(j,pl.Get(c))
		pl.Set(c,k)
	Next
	Log("End Shufflelist Function")
	Return pl
End Sub

